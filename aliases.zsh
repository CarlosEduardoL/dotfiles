# Tooling aliases
alias vi='lvim' # You can change it to nvim or remove it if you want
alias ls='exa --icons'
alias cat='bat'

# ls aliases
alias ll='ls -l'
alias la='ll -a'
alias sl='ls' # Fix typos

# Utility aliases
alias reload='exec zsh' # You can just source your .zshrc or .zshenv if you want
